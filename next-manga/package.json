{
  "name": "@monorepo/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-w-11": "next build --openssl-legacy-provider",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook",
    "generate": "yarn plop --plopfile generators/plopfile.js"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@headlessui/react": "^1.6.4",
    "@styled-icons/bootstrap": "^10.34.0",
    "@styled-icons/boxicons-logos": "^10.38.0",
    "@styled-icons/boxicons-regular": "^10.38.0",
    "@styled-icons/boxicons-solid": "^10.38.0",
    "@styled-icons/entypo": "^10.34.0",
    "@styled-icons/entypo-social": "^10.34.0",
    "@styled-icons/evaicons-outline": "^10.34.0",
    "@styled-icons/fa-regular": "^10.34.0",
    "@styled-icons/material-outlined": "^10.34.0",
    "@styled-icons/open-iconic": "^10.18.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "@types/lodash.xor": "^4.5.6",
    "@types/react-slick": "^0.23.5",
    "dayjs": "^1.10.7",
    "graphql": "^15.6.1",
    "joi": "^17.4.2",
    "lodash.xor": "^4.5.0",
    "next": "12.2.5",
    "next-auth": "^3.29.0",
    "next-pwa": "^5.3.1",
    "nextjs-progressbar": "^0.0.11",
    "phosphor-react": "^1.4.1",
    "polished": "^4.1.3",
    "react": "18",
    "react-bottom-scroll-listener": "^5.0.0",
    "react-dom": "18",
    "react-slick": "^0.28.1",
    "sharp": "^0.29.2",
    "storybook-addon-next-router": "^3.0.8",
    "styled-components": "^5.3.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/styled-components": "^5.1.14",
    "apollo": "^2.33.6",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.1.2",
    "plop": "^2.7.4",
    "postcss": "^8.4.14",
    "tailwind-scrollbar": "^1.3.1",
    "tailwind-styled-components": "^2.1.7",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  }
}
